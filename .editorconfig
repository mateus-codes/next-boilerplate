# top-most EditorConfig file
root = true

# 2 space indentation
# Unix-style newlines with a newline ending every file
# Set default charset
# Remove any whitespace characters preceding newline characters 
[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true


